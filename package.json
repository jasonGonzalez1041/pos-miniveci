{
  "name": "pos-miniveci",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build:worker && node scripts/dev-server.js",
    "dev:next": "npm run build:worker && next dev",
    "build": "npm run build:worker && next build",
    "build:worker": "node scripts/compile-worker-simple.js",
    "postbuild": "cp public/_headers out/_headers 2>/dev/null || copy public\\_headers out\\_headers >NUL 2>&1 || true",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/lib/db/seed.ts",
    "migrate:images": "tsx scripts/migrate-images.ts",
    "sync:woo": "tsx scripts/sync-woo-manual.ts",
    "test:r2": "tsx scripts/test-r2-connection.ts",
    "build:cf": "next build",
    "pages:deploy": "npx wrangler pages deploy out --project-name=pos-miniveci",
    "preview": "npx wrangler pages dev out --port 3001",
    "verify:cf": "ts-node scripts/verify-cloudflare-deployment.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
