    "lucide-react": "^0.553.0",
    "next": "16.0.0",
    "next-themes": "^0.4.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.66.0",
    "react-hotkeys-hook": "^4.4.4",
    "sharp": "^0.33.2",
    "sonner": "^2.0.7",
